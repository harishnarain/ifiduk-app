{
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true,
        "node": true,
        "jest": true
    },
    "globals": {
        "$": true
    },
    "plugins": [
        "prettier",
        "react",
        "react-hooks"
    ],
    "extends": [
        "prettier",
        "prettier/react",
        "airbnb"
    ],
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }],
        "indent": ["off", 2],
        "react/jsx-indent": ["off", 2],
        "operator-linebreak": ["off"],
        "import/imports-first": ["error", "absolute-first"],
        "import/newline-after-import": "error",
        "no-unused-vars": ["warn"],
        "react/prop-types": 0,
        "no-console": 0,
        "no-alert": 0,
        "object-curly-newline": 0
    }
}
