{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "prettier/react",
        "airbnb"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier",
        "react-hooks"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }],
        "import/imports-first": ["error", "absolute-first"],
        "import/newline-after-import": "error",
        "react/prop-types": 0,
        "no-console": 0
    }
}
